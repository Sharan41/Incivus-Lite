# Production Environment Variables for Incivus UI
# These should be set in Cloud Run environment

# Main API URL for app.py (database operations) - CHANGE THESE TO YOUR PRODUCTION URLS
REACT_APP_URL=https://your-middleware-url.run.app

# Analysis API URL for main.py (model operations) - CHANGE THESE TO YOUR PRODUCTION URLS
REACT_APP_API_URL=https://your-backend-url.run.app

# Python API Configuration
REACT_APP_USE_MOCK_API=false
REACT_APP_PYTHON_API_TIMEOUT=120000

# Build Configuration
NODE_ENV=production