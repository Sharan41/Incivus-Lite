# Production Environment Variables for Incivus UI
# These should be set in Cloud Run environment

# Main API URL for app.py (database operations) - Port 8002
REACT_APP_URL=http://localhost:8002/

# Analysis API URL for main.py (model operations) - Port 8000
REACT_APP_API_URL=http://localhost:8000/

# Python API Configuration
REACT_APP_USE_MOCK_API=false
REACT_APP_PYTHON_API_TIMEOUT=120000

# Build Configuration
NODE_ENV=production